<% if(is_category()){ %>
    <%- partial('_page/category', {pagination: config.category, index: true}) %>
<% }else{ %>
    <%
    let virtualPage = {
        path: 'categories/index.html',
        category: 'All Categories'
    };
    %>
    
    <%- partial('_page/category', {page: virtualPage}) %>
<% } %>